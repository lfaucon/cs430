	</agent>

	<agent name="reactive-.5">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveMDP"/>
		<set discount-factor="0.5"/>
	</agent>

	<agent name="reactive-.25">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveMDP"/>
		<set discount-factor="0.25"/>
	</agent>

	<agent name="reactive-.01">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveMDP"/>
		<set discount-factor="0.01"/>
	</agent>

</agents>
